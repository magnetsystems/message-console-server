        <!-- Spots -->

        <resource path="spots">
            <method id="spots_list" name="GET" apigee:displayName="List">
                <apigee:tags>
                    <apigee:tag primary="true">Spots</apigee:tag>
                </apigee:tags>

                <apigee:authentication required="false"/>

                <apigee:example url="spots"/>

                <doc title="spots_list" apigee:url="http://gowalla.com/api/explorer#/spots?lat=&amp;lng=&amp;radius=">
                    <![CDATA[Retrieve a list of spots within a specified distance of a location.]]>
                </doc>

                <request>
                    <!-- Common parameters -->

                    <param name="Accept" style="header" required="true" default="application/json">
                        <doc apigee:url="http://gowalla.com/api/docs#format">
                            <![CDATA[Specifies the response format. Most API methods provide JSON responses, served as application/json, encoded as UTF-8 (default). In some cases, records are available in additional formats, like Atom or HTML. You can retrieve records in these formats by changing the Accept header as needed.]]>
                        </doc>
                        <option value="application/json"/>
                        <option value="application/atom+xml"/>
                        <option value="text/html"/>
                    </param>

                    <param name="page" type="xsd:positiveInteger" style="query" required="false">
                        <doc apigee:url="http://gowalla.com/api/docs#pagination">
                            <![CDATA[Specifies which specific page in the set to request.]]>
                        </doc>
                    </param>

                    <param name="per_page" type="xsd:positiveInteger" style="query" required="false">
                        <doc apigee:url="http://gowalla.com/api/docs#pagination">
                            <![CDATA[Specifies the number of results per page to return.]]>
                        </doc>
                    </param>

                    <!-- Method specific -->

                    <param name="lat" type="xsd:double" style="query" required="true">
                        <doc apigee:url="http://gowalla.com/api/explorer#/spots?lat=&amp;lng=&amp;radius=">
                            <![CDATA[Latitude of the location around which the spots will be returned.]]>
                        </doc>
                    </param>

                    <param name="lng" type="xsd:double" style="query" required="true">
                        <doc apigee:url="http://gowalla.com/api/explorer#/spots?lat=&amp;lng=&amp;radius=">
                            <![CDATA[Longitude of the location around which the spots will be returned.]]>
                        </doc>
                    </param>

                    <param name="radius" type="xsd:double" style="query" required="true">
                        <doc apigee:url="http://gowalla.com/api/explorer#/spots?lat=&amp;lng=&amp;radius=">
                            <![CDATA[Search radius (in meters) for the location around which the spots will be returned.]]>
                        </doc>
                    </param>
                </request>

                <response>
                    <representation mediaType="application/json"/>
                    <representation mediaType="application/atom+xml"/>
                    <representation mediaType="text/html"/>
                </response>
            </method>
        </resource>

