<script id="PWCoreView" type="text/template">
<div did="core" class="accordion" id="core-accordion">
<div did="encryption" class="accordion-group">
    <div class="accordion-heading">
        <a class="accordion-toggle collapsed" data-toggle="collapse" data-parent="#core-accordion" href="#core-encryption">
            Encryption Services
            <i class="icon icon-chevron-down"></i>
            <div class="preview-status">
                <% if(project.attributes['encryptionEnabled'] == true){ %>ON<% }else{ %>OFF<% } %>
            </div>
        </a>
    </div>
    <div id="core-encryption" class="accordion-body collapse" style="height:0">
        <div class="accordion-inner">
            <div class="clearfix">
                <div did="ON" class="<% if(project.attributes['encryptionEnabled'] !== true){ %>hidden<% } %>">
                    You currently have enabled Android encryption services for cached data.
                </div>
                <div did="OFF" class="<% if(project.attributes['encryptionEnabled'] == true){ %>hidden<% } %>">
                    You currently have not enabled Android encryption services for cached data.
                </div>
                <div did="encryptionEnabled" class="btn-group">
                    <button did="true" class="btn <% if(project.attributes['encryptionEnabled'] == true){ %>btn-primary<% } %>">ON</button>
                    <button did="false" class="btn <% if(project.attributes['encryptionEnabled'] !== true){ %>btn-primary<% } %>">OFF</button>
                </div>
            </div>
            <div class="clearfix">
                <div>
                    Every iOS device has built in encryption services<br /> used to encrypt all data on the device at all times.

                    <div class="centered-text">
                        Always On
                    </div>
                </div>
            </div>
            <div class="clearfix">
                <div>
                    Encryption of cached data on the server is always enabled.
                    <div class="centered-text">
                        Always On
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div did="locationdata" class="accordion-group">
    <div class="accordion-heading">
        <a class="accordion-toggle collapsed" data-toggle="collapse" data-parent="#core-accordion" href="#core-datacollection">
            Location Data Collection
            <i class="icon icon-chevron-down"></i>
            <div class="preview-status">
                <% if(project.attributes['useGeoLocation'] == true){ %>ON<% }else{ %>OFF<% } %>
            </div>
        </a>
    </div>
    <div id="core-datacollection" class="accordion-body collapse" style="height:0">
        <div class="accordion-inner">
