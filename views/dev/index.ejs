<script id="SandboxDetailsView" type="text/template">
    <table class="table table-bordered table-striped">
    <% if(cloudCompute.attributes.deployedApplicationStatus === 'complete'){ %>
        <tr>
            <td width="50%">Deployment Status</td>
            <td>
                <strong class="cloud-state" style="color:green">RUNNING</strong>
            </td>
        </tr>
        <tr>
            <td>Sandbox URL</td>
            <td>
                <a target="_blank" href="<%- project.attributes.sandboxUrl %>"><%- project.attributes.sandboxUrl %></a>
            </td>
        </tr>
        <% if(project.attributes.sandboxUser){ %>
        <tr>
            <td>Sandbox Username</td>
            <td>
                <b>
                    <%- project.attributes.sandboxUser %>
                </b>
            </td>
        </tr>
        <% } %>
        <% if(project.attributes.sandboxPassword){ %>
        <tr>
            <td>Sandbox Password</td>
            <td>
                <b><%- project.attributes.sandboxPassword %>
                </b>
            </td>
        </tr>
        <% } %>
        <tr>
            <td>Time of Deployment</td>
            <td><%- cloudCompute.attributes.launchTime ? utils.ISO8601ToDT(cloudCompute.attributes.launchTime) : '' %></td>
        </tr>
        <tr>
            <td>Name</td>
            <td><%- cloudCompute.attributes.deployedApplicationName %></td>
        </tr>
        <tr>
            <td>Base Image Name</td>
            <td><%- cloudCompute.attributes.baseImageName %></td>
        </tr>
    <% }else if(cloudCompute.attributes.deployedApplicationStatus === 'started'){ %>
        <tr>
            <td width="50%">Deployment Status</td>
            <td>
                <strong class="cloud-state" style="color:#D6AE00">DEPLOYING - Step <%- deployStage %> of 3</strong><br />
                <% if(deployStage == 1){ %>
                    Provisioning sandbox to the cloud. Do not
                    leave this page until this step completes.
                <% } %>
                <% if(deployStage == 2){ %>
                    Uploading server to sandbox. This step may
                    take a few minutes. It is safe to leave this
                    page and deployment will continue.
                <% } %>
                <% if(deployStage == 3){ %>
                    Launching server in sandbox. This step may take a few minutes.
                <% } %>
                <br />
                <img src="../images/assets/progress-loading.gif" />
            </td>
        </tr>
    <% }else if(cloudCompute.attributes.deployedApplicationStatus === 'failed'){ %>
        <tr>
            <td width="50%">Deployment Status</td>
            <td>
                <strong class="cloud-state" style="color:red">FAILED</strong>
                <p>This project failed during deployment. However, you can still run the server locally using the generated server binary.</p>
                <p>Please view the debugging console on the right for server output or <a href="/support/">contact Magnet support</a> for assistance.</p>
                <button class="sendSupport btn" info="error deploying to cloud">Send Error Report</button>
            </td>
        </tr>
    <% } %>
    </table>
    <h4>Sandbox Administration</h4>
    <table class="table table-bordered table-striped">
        <tr>
            <td>Re-Deploy your project to the cloud.</td>
            <td width="50%">
                <button id="deploy-sandbox" class="btn btn-primary <% if(cloudCompute.attributes.deployedApplicationStatus === 'started'){ %>disabled<% } %>">Re-Deploy to Cloud</button>
            </td>
        </tr>
    </table>
</script>

