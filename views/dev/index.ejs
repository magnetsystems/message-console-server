                        <button did="false" class="btn <% if(project.attributes['facebookEnabled'] !== true){ %>btn-primary<% } %>">Don't Include</button>
                    </div>
                    <div did="ON" class="<% if(project.attributes['facebookEnabled'] !== true){ %>hidden<% } %>">
                        Your project is including this third party service.
                    </div>
                    <div did="OFF" class="<% if(project.attributes['facebookEnabled'] == true){ %>hidden<% } %>">
                        Your project currently does NOT include this third party service.
                    </div>
                    <div id="thirdparty-facebook-fields" class="optional-parameters <% if(project.attributes['facebookEnabled'] !== true){ %>disabled<% } %>">
                        <div class="title">
                            Parameters
                        </div>
                        <div class="content">
                            <dl class="dl-horizontal">
                                <dt>Credentials</dt>
                                <dd>
                                    <div class="controls controls-row">
                                        <input class="span5" type="text" name="fbClientId" value="<%- project.attributes['fbClientId'] %>" placeholder="App ID/API Key">
                                        <input class="span5" type="text" name="fbClientSecret" value="<%- project.attributes['fbClientSecret'] %>" placeholder="App Secret">
                                    </div>
                                </dd>
                            </dl>
                            <p>
                                <b>Note:</b> If you do not have the information above, please visit:
                            </p>
                            <p>
                                <a href="https://developers.facebook.com/apps" target="_blank">Facebook Developers website to register your app and obtain authentication keys</a>
                            </p>
                        </div>
                        <% if(project.attributes['facebookEnabled'] !== true){ %>
                        <div class="overlay"></div>
                        <% } %>
                    </div>
                </div>
            </div>
        </div>
    </div>

</script>
