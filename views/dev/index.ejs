<script id="PWWSDLListView" type="text/template">
    <% if(!webservices || webservices.length == 0){ %>
    <% }else{ %>
    <% _.each(webservices, function(webservice, id){ %>
    <li did="<%- webservice.magnetId %>">
        <strong>
            <% if(webservice.type){ %>
            <%- webservice.type %>
            <% }else{ %>
            WSDL
            <% } %>
        </strong>
        <% if(webservice.url){ %>
        <% if(webservice.url.length > 22){ %>
        <%- webservice.url.substr(0, 22) %>...
        <% }else{ %>
        <%- webservice.url %>
        <% } %>
        <% }else{ %>
        <%- webservice.name %>
        <% } %>
        <% if(webservice.policies){ %>
        <span class="greentxt">Using WS Policy</span>
        <% }else{ %>
        <span style="color:#cc0000;margin-left:17px">No WS Policy detected. Select a setting below:</span>
        <% } %>
        <i class="icon-remove-sign"></i>
        <% if(!webservice.policies){ %>
        <div class="ws-policy">
            <table class="radio-collection">
                <tr did="default">
                    <td class="first">
                        <input type="radio" did="BasicAuth" name="authMethod-<%- webservice.id %>" value="BasicAuth" <% if(webservice.authMethod == 'BasicAuth'){ %>checked="checked"<% } %> /> Basic Auth
                    </td>
                    <td>
                        Choose this option to enable default user and login to work in Magnet's sandbox database.
                        <div did="BasicAuth" class="well radio-toggle <% if(webservice.authMethod != 'BasicAuth'){ %>hidden<% } %>">
                            <input type="text" name="serviceAccountUserName" <% if(webservice.authMethod == 'BasicAuth'){ %>value="<%- webservice.serviceAccountUserName %>"<% } %> placeholder="ID">
                            <input type="text" name="serviceAccountPassword" <% if(webservice.authMethod == 'BasicAuth'){ %>value="<%- webservice.serviceAccountPassword %>"<% } %> placeholder="Password">
                            <div did="useTimestamp" class="btn-group">
                                <button did="true" class="btn <% if(webservice.useTimestamp === true || webservice.useTimestamp == true){ %>btn-primary<% } %>">Timestamp ON</button>
                                <button did="false" class="btn <% if(webservice.useTimestamp !== true && webservice.useTimestamp != 'true'){ %>btn-primary<% } %>">Timestamp OFF</button>
                            </div>
                            <p>You may leave Username and Password blank if you want to specify them later.</p>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td class="first">
                        <input type="radio" did="UsernameToken" name="authMethod-<%- webservice.id %>" value="UsernameToken" <% if(webservice.authMethod == 'UsernameToken'){ %>checked="checked"<% } %> /> UsernameToken
                    </td>
                    <td>
                        Your project will use your own Database for user authentication.
                        <div did="UsernameToken" class="well radio-toggle <% if(webservice.authMethod != 'UsernameToken'){ %>hidden<% } %>">
                            <input type="text" name="serviceAccountUserName" <% if(webservice.authMethod == 'UsernameToken'){ %>value="<%- webservice.serviceAccountUserName %>"<% } %> placeholder="ID">
                            <input type="text" name="serviceAccountPassword" <% if(webservice.authMethod == 'UsernameToken'){ %>value="<%- webservice.serviceAccountPassword %>"<% } %> placeholder="Password">
                            <div did="passwordType" class="btn-group">
                                <button did="TEXT" class="btn <% if(webservice.passwordType === 'TEXT'){ %>btn-primary<% } %>">cleartext</button>
                                <button did="DIGEST" class="btn <% if(webservice.passwordType !== 'TEXT'){ %>btn-primary<% } %>">digest</button>
                            </div>
                            <div did="useTimestamp" class="btn-group">
                                <button did="true" class="btn <% if(webservice.useTimestamp === true || webservice.useTimestamp == true){ %>btn-primary<% } %>">Timestamp ON</button>
                                <button did="false" class="btn <% if(webservice.useTimestamp !== true && webservice.useTimestamp != 'true'){ %>btn-primary<% } %>">Timestamp OFF</button>
                            </div>
                            <p>You may leave ID and Password blank if you want to specify them later.</p>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td class="first">
                        <input type="radio" did="none" name="authMethod-<%- webservice.id %>" value="none" <% if(webservice.authMethod != 'UsernameToken' && webservice.authMethod != 'BasicAuth'){ %>checked="checked"<% } %> /> None
                    </td>
                    <td>
                        This service does not require security information.
                    </td>
                </tr>
                <tr>
                    <td class="first">
                        <input type="radio" did="specifyLater" name="authMethod-<%- webservice.id %>" value="specifyLater" <% if(webservice.authMethod == 'specifyLater'){ %>checked="checked"<% } %> /> Specify later
                    </td>
                    <td>
                        Your project will use your own Active Directory for user authentication.
                        <div did="specifyLater" class="well radio-toggle <% if(webservice.authMethod != 'specifyLater'){ %>hidden<% } %>">
                            <b>Note:</b> If your web service requires authentication, then you must
                            configure security settings or your app may not communicate property
                            with this web service.
                        </div>
                    </td>
                </tr>
            </table>
        </div>
        <hr />
        <div class="row-fluid">
            <div class="span5">
                <input type="text" class="span12" name="pathPrefix" placeholder="Path Prefix" value="<%- webservice.pathPrefix %>" />
            </div>
            <div class="span7">
                <div>Optionally set a prefix for all of the APIs exposed by this web service to prevent conflicts.</div>
            </div>
        </div>
        <% } %>
    </li>
    <% }); %>
    <% } %>
</script>

