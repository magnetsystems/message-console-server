<script id="accountSettingsView" type="text/template">
    <h1>Account Settings</h1>
    <div id="account-form-container">
        <form id="account-form" class="form-horizontal well" style="width:500px">
            <h1></h1>
            <p id="sub1" class="subheading">
                Please tell us a little about yourself.
            </p>
            <hr/>
            <fieldset>
                <div id="name-cg" class="control-group">
                    <label for="name-tf" class="control-label">Name</label>
                    <div class="controls">
                        <input id="name-tf" type="text" name="name" value="<%= name %>" class="input-xlarge"/>
                    </div>
                </div>
                <div id="email-cg" class="control-group">
                    <label for="email-tf" class="control-label">Email</label>
                    <div class="controls">
                        <input id="email-tf" type="text" name="email" value="<%= email %>" class="input-xlarge"/>
                    </div>
                </div>
                <div id="clist-cg" class="control-group">
                    <label for="country-list" class="control-label">Location</label>
                    <div class="controls">
                        <select id="country-list" name="country">
                            <% for(var i in countries){ %>
                            <option value="<%= countries[i].short %>"<% if(countries[i].short == country){ %> selected<% } %>><%= countries[i].name %></option><% } %>
                        </select>
                    </div>
                </div>
                <hr/>
                <div class="control-group">
                    <label for="opass-tf" class="control-label">Old Password</label>
                    <div class="controls">
                        <input id="opass-tf" type="password" name="oldpass" value="" class="input-xlarge"/>
                    </div>
                </div>
                <div class="control-group">
                    <label for="npass-tf" class="control-label">New Password</label>
                    <div class="controls">
                        <input id="npass-tf" type="password" name="newpass" value="" class="input-xlarge"/>
                    </div>
                </div>
                <div class="control-group">
                    <label for="del-acct" class="control-label">Delete Account</label>
                    <div class="controls">
                        <button type="button" class="btn-danger btn">Delete My Account</button>
                    </div>
                </div>
                <div class="form-actions">
                    <button type="button" class="btn-primary btn">Save Changes</button>
                </div>
            </fieldset>
        </form>
    </div>
</script>
<script id="createDayTripView" type="text/template">
    <h1>Create a DayTrip</h1>
    <p id="sub1" class="subheading">
        Lets create a DayTrip! A DayTrip is composed of a combination of places. When you have created your DayTrip, you can easily share it with friends and family.
    </p>
    <hr/>
    <div class="row-fluid">
        <div id="createDayTripTitle" class="span6">
            <legend>1. Enter a Title</legend>
            <p>
                Enter a title so you can remember your awesome daytrip.
            </p>
            <form class="form-search">
                <input type="text" name="title" value="" class="input-xlarge"/>
                <button type="button" class="btn btn-primary">Save</button>
            </form>
        </div>
        <div id="createDayTripArea" class="span6">
            <legend>2. Enter an Area</legend>
            <p>
                Enter a general area which contains all of your places.
            </p>
            <form class="form-search">
                <input type="text" name="area" value="" class="input-xlarge"/>
                <button type="button" class="btn btn-primary">Search</button>
            </form>
        </div>
    </div>
    <div class="row-fluid">
        <span id="locations" class="hide">
            <div class="span6">
                <legend>3. Select a Location</legend>
                <table class="table">
                </table>
            </div>
            <div id="area-map" class="span6">
            </div>
        </span>
    </div>
    <div class="row-fluid">
        <div id="select-place" class="span6 hide">
            <legend>4a. Add Some Places</legend>
            <p>
                Select a place to see more information about it.
            </p>
            <form class="form-search">
                <input type="text" name="search" value="" class="input-xlarge"/>
                <button type="button" class="btn btn-primary">Search</button>
            </form>
            <div id="place-info"></div>
        </div>
        <div id="places-list" class="span6 hide">
            <legend>4b. Your Places</legend>
            <div class="sequential-list"></div>
        </div>
    </div>
</script>
<script id="locationsListView" type="text/template">
      <thead>
        <tr>
          <th>Name</th>
          <th>Location</th>
        </tr>
      </thead>
      <tbody>
      <% _.each(locations, function(loc, key){ %>
        <tr data-id="<%- key %>" lat="<%- loc.latlong.lat %>" long="<%- loc.latlong.lng %>">
          <td><%- loc.name %></td>
          <td><%- loc.addr %></td>
        </tr>
      <% }); %>
      </tbody>
</script>
<script id="placeInfoView" type="text/template">
<% _.each(places, function(place, key){ %>
    <div data-id="<%- key %>" gid="<%- place.id %>" lat="<%- place.latlong.lat %>" long="<%- place.latlong.lng %>" class="well oneplace">
        <img src="<%- place.icon %>" class="place-icon" />
        <dl class="dl-horizontal">
            <dt>Name</dt>
            <dd><b><span class="place-name"><%- place.name %></span></b></dd>
            <dt>Location</dt>
            <dd><%- place.vicinity %></dd>
            <dt>Types</dt>
            <dd><%- place.types %></dd>
            <dt>Rating</dt>
            <dd><%- place.rating %></dd>
        </dl>
    </div>
<% }); %>
</script>
<script id="placesListView" type="text/template">
<% _.each(places, function(place, key){ %>
<% if(key > 0){ %>
        <div class="list-sep">
            <img src="/img/list-sep.png" />
            <div class="list-info"><%- place.distance %></div>
            <img src="/img/list-sep.png" />
        </div>
<%}%>
        <div data-id="<%- key %>" gid="<%- place.gid %>" class="well">
            <%- place.name %> <b><span class="place-remove">[x]</span></b>
        </div>
<% }); %>
</script>

