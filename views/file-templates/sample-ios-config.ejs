<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>default</key>
        <dict>
            <key>BaseURL</key>
            <string>mmx://{{=mmxconfig.configs['mmx.push.callback.host'] || ''}}:5222</string>
            <key>DomainName</key>
            <string>{{=mmxconfig.configs['xmpp.domain'] || 'mmx'}}</string>
            <key>AppId</key>
            <string>{{=params.appId || ''}}</string>
            <key>ApiKey</key>
            <string>{{=params.appAPIKey || ''}}</string>
            <key>AnonymousSecret</key>
            <string>{{=params.guestSecret || ''}}</string>
            <key>ShouldForceTLS</key>
            <true/>
            <key>PublicAPIPort</key>
            <integer>{{=mmxconfig.configs['mmx.rest.http.port'] || 5220}}</integer>
            <key>AllowInvalidCertificates</key>
            <true/>
        </dict>
    </dict>
</plist>
